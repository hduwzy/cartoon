<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.cartoon.categorysecond.domain">
	<class name="CategorySecond" table="categorysecond">
		<id name="csid">
			<generator class="native"></generator>
		</id>
		<property name="csname"></property>
		<many-to-one name="category" class="cn.itcast.cartoon.category.domain.Category" column="cid"></many-to-one>
	</class>
</hibernate-mapping>